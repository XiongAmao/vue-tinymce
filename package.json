{
  "name": "@panhezeng/vue-tinymce",
  "description": "A Vue component",
  "version": "2.1.9",
  "author": "潘何增 <panhezeng@gmail.com> (http://apsay.com/)",
  "license": "MIT",
  "private": false,
  "keywords": [
    "vuejs",
    "vue component",
    "tinymce",
    "rich text editor"
  ],
  "homepage": "https://github.com/panhezeng/vue-tinymce.git#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/panhezeng/vue-tinymce.git"
  },
  "files": [
    "dist",
    "src",
    "package.json",
    "README"
  ],
  "main": "dist/vue-tinymce.min.js",
  "scripts": {
    "dev:example": "webpack-dev-server --mode development --open --hot --config webpack.example.config.js",
    "build:example": "webpack --mode production --config webpack.example.config.js",
    "build": "webpack --mode production",
    "publish": "npm set registry https://registry.npmjs.org/ && npm set @panhezeng:registry https://registry.npmjs.org/ && npm publish --access public && npm set registry https://registry.npm.taobao.org/ && npm set @panhezeng:registry https://registry.npm.taobao.org/",
    "publish:patch": "npm set registry https://registry.npmjs.org/ && npm set @panhezeng:registry https://registry.npmjs.org/ && npm version patch && npm publish --access public && npm set registry https://registry.npm.taobao.org/ && npm set @panhezeng:registry https://registry.npm.taobao.org/"
  },
  "peerDependencies": {
    "tinymce": "^5.0.11",
    "vue": "^2.6.0"
  },
  "dependencies": {
    "npm": "^6.13.1"
  },
  "devDependencies": {
    "@babel/core": "^7.7.4",
    "@babel/plugin-syntax-dynamic-import": "^7.7.4",
    "@babel/plugin-transform-runtime": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "@babel/runtime": "^7.7.4",
    "@panhezeng/before-run-webpack-plugin": "^1.0.10",
    "babel-loader": "~8",
    "css-loader": "^3.2.0",
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1",
    "tinymce": "^5.1.2",
    "vue": "^2.6.10",
    "vue-loader": "^15.7.2",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md,jsx,vue,ts,less,html}": [
      "prettier --write",
      "git add"
    ]
  },
  "engines": {
    "node": ">= 10.0.0",
    "npm": ">= 6.0.0"
  }
